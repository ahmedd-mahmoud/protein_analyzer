# Example environment configuration for Protein Analyzer
# Copy this file to .env and modify as needed

# API Configuration
NCBI_TIMEOUT=30
ALPHAFOLD_TIMEOUT=30
REQUEST_DELAY=1.0

# File Processing
CHUNK_SIZE=8192
MAX_RETRIES=3

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=protein_analyzer.log

# Output Configuration
DEFAULT_OUTPUT_DIR=./output
EXCEL_FILENAME=analysis_results.xlsx

# Species Classification Thresholds
GENERAL_SPECIES_THRESHOLD=100
SPECIFIC_SPECIES_THRESHOLD=10

# PLDDT Highlighting Threshold
PLDDT_HIGHLIGHT_THRESHOLD=70